{
    "name": "chepchepcirkus/VarnishBakery",
    "description": "VarnishBakery plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "BSD 3-Clause",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "cakephp/cakephp-codesniffer": "*",
        "php-coveralls/php-coveralls": "dev-master",
        "phpunit/phpcov": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "VarnishBakery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VarnishBakery\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
